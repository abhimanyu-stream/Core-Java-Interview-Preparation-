package com.corejava8.learning;

public class Customer {
	
	private Long id;
	private String name;
	private Double price;
	private OrderType orderType;
	
	public Customer() {}
	public Customer(Long id, String name, Double price, OrderType orderType) {
		super();
		this.id = id;
		this.name = name;
		this.price = price;
		this.orderType = orderType;
	}
	
	
	
	
	
	
	

}
